  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "db:seed": "tsx prisma/seed.ts",
    "clean": "rimraf .next node_modules",
    "deploy": "vercel --prod --yes",
    "auto:commit": "pwsh -File scripts/auto-commit.ps1",
    "auto:heal": "pwsh -File scripts/auto-heal.ps1",
    "auto:fix": "npm run lint:fix && npm run format",
    "auto:all": "npm run auto:fix && git add -A && git commit -m 'ðŸ¤– Auto: fix and format' && git push && npm run deploy",
    "generate:content": "tsx scripts/generate-content.ts",
    "generate:revenue": "tsx scripts/generate-revenue-pages.ts",
    "verify:sync": "bash scripts/verify-sync.sh",
    "ping:search": "tsx scripts/ping-search.ts",
    "verify:revenue": "tsx scripts/verify-revenue-lock.ts"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
