{
  "name": "3000studios-next",
  "version": "1.0.1",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@10.26.0",
  "overrides": {
    "camera-controls": "3.1.1"
  },
  "pnpm": {
    "overrides": {
      "camera-controls": "3.1.1"
    }
  },
  "scripts": {
    "autosync": "./antigravity-autosync.sh",
    "analyze": "cross-env ANALYZE=true npm run build",
    "dev": "next dev",
    "harden": "bash scripts/harden-repo.sh",
    "prebuild": "node scripts/validate-app-structure.mjs",
    "build": "next build --webpack",
    "start": "next start",
    "lint": "eslint .",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "prune:types": "ts-prune --error",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "db:seed": "tsx prisma/seed.ts",
    "clean": "rimraf .next node_modules",
    "clean:deps": "rimraf node_modules package-lock.json",
    "fresh:install": "npm run clean:deps && npm install",
    "deps:audit": "npm audit fix",
    "deps:update": "npm update",
    "deploy": "vercel --prod --yes",
    "postinstall": "node -e \"const { execSync } = require('child_process'); const url = process.env.DATABASE_URL || 'postgresql://postgres:postgres@localhost:5432/postgres'; execSync('npx prisma generate', { stdio: 'inherit', env: { ...process.env, DATABASE_URL: url } });\"",
    "slicemachine": "start-slicemachine"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.89",
    "@anthropic-ai/sdk": "^0.71.2",
    "@auth/prisma-adapter": "^2.11.1",
