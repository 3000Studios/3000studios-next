{
  "name": "3000studios-next",
  "version": "1.0.1",
  "private": true,
  "type": "module",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "db:seed": "tsx prisma/seed.ts",
    "clean": "rimraf .next node_modules",
    "clean:deps": "rimraf node_modules package-lock.json",
    "fresh:install": "npm run clean:deps && npm install",
    "deps:audit": "npm audit fix",
    "deps:update": "npm update",
    "deploy": "vercel --prod --yes",
    "postinstall": "node -e \"const { execSync } = require('child_process'); const url = process.env.DATABASE_URL || 'postgresql://postgres:postgres@localhost:5432/postgres'; execSync('npx prisma generate', { stdio: 'inherit', env: { ...process.env, DATABASE_URL: url } });\""
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.88",
    "@anthropic-ai/sdk": "^0.71.2",
    "@auth/prisma-adapter": "^2.11.1",
    "@google/generative-ai": "^0.24.1",
    "@mux/mux-node": "^12.8.1",
    "@mux/mux-player-react": "^3.10.1",
    "@octokit/rest": "^22.0.1",
    "@paypal/paypal-server-sdk": "^2.1.0",
    "@paypal/react-paypal-js": "^8.9.2",
    "@prisma/client": "^6.19.1",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.4.2",
    "@stripe/stripe-js": "^8.5.3",
    "@types/pg": "^8.16.0",
    "@types/uuid": "^10.0.0",
    "@vercel/analytics": "^1.6.1",
    "ai": "^5.0.115",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "formidable": "^3.5.4",
    "framer-motion": "^12.23.25",
    "howler": "^2.2.4",
    "jsonwebtoken": "^9.0.3",
    "lucide-react": "^0.561.0",
    "next": "^16.0.10",
    "next-auth": "^5.0.0-beta.30",
    "nodemailer": "^7.0.11",
    "openai": "^6.14.0",
    "pg": "^8.16.3",
    "pgvector": "^0.2.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "rimraf": "^6.1.2",
    "stripe": "^20.0.0",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.182.0",
    "uuid": "^13.0.0",
    "zod": "^4.2.1",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/bcryptjs": "^3.0.0",
    "@types/google.maps": "^3.58.1",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.0.1",
    "@types/nodemailer": "^6.4.16",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/ui": "^4.0.16",
    "eslint": "^9",
    "eslint-config-next": "16.0.10",
    "jsdom": "22.1.0",
    "prisma": "^6.19.1",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5",
    "vitest": "^4.0.16"
  }
