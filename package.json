    "@google/generative-ai": "^0.24.1",
    "@mediapipe/face_mesh": "^0.4.1633559619",
    "@monaco-editor/react": "^4.7.0",
    "@mux/mux-node": "^12.8.1",
    "@mux/mux-player-react": "^3.10.2",
    "@octokit/rest": "^22.0.1",
    "@paypal/paypal-server-sdk": "^2.1.0",
    "@paypal/react-paypal-js": "^8.9.2",
    "@prisma/client": "^6.19.1",
    "@prismicio/client": "^7.21.2",
    "@prismicio/next": "^2.0.2",
    "@prismicio/react": "^3.2.2",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.5.0",
    "@stripe/stripe-js": "^8.6.0",
    "@tailwindcss/postcss": "4.1.18",
    "@types/diff": "^8.0.0",
    "@types/pg": "^8.16.0",
    "@types/three": "^0.182.0",
    "@types/uuid": "^10.0.0",
    "ai": "^5.0.117",
    "autoprefixer": "10.4.23",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "canvas-confetti": "^1.9.4",
    "clsx": "^2.1.1",
    "diff": "^8.0.2",
    "dotenv": "^17.2.3",
    "formidable": "^3.5.4",
    "framer-motion": "^12.23.26",
    "gsap": "^3.14.2",
    "howler": "^2.2.4",
    "jsonwebtoken": "^9.0.3",
    "lucide-react": "^0.561.0",
    "monaco-editor": "^0.55.1",
    "mongodb": "^7.0.0",
    "next": "^16.1.1",
    "next-auth": "^4.24.13",
    "nodemailer": "^7.0.12",
    "npm-run-all": "^4.1.5",
    "openai": "^6.15.0",
    "pg": "^8.16.3",
    "pgvector": "^0.2.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "rimraf": "^6.1.2",
    "simple-git": "^3.30.0",
    "stripe": "^20.1.0",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.182.0",
    "uuid": "^13.0.0",
    "zod": "^3.24.1",
    "zustand": "^4.5.7"
